{
  "name": "bookclub-backend",
  "version": "1.0.0",
  "description": "Serverless backend for BookClub application",
  "main": "index.js",
  "scripts": {
    "deploy": "serverless deploy",
    "deploy:dev": "serverless deploy --stage dev",
    "deploy:prod": "serverless deploy --stage prod",
    "offline": "SLS_TELEMETRY_DISABLED=1 serverless offline start",
    "dev": "SLS_TELEMETRY_DISABLED=1 IS_OFFLINE=true NODE_ENV=development AWS_ACCESS_KEY_ID=local AWS_SECRET_ACCESS_KEY=local AWS_REGION=us-east-1 AWS_DEFAULT_REGION=us-east-1 AWS_SDK_LOAD_CONFIG=0 serverless offline start --config serverless-offline.yml --stage dev",
    "seed": "node scripts/seed-data.js",
    "dev:seed": "npm run seed && npm run dev",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "serverless",
    "aws",
    "lambda",
    "bookclub"
  ],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "aws-sdk": "^2.1692.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "serverless": "^3.38.0",
    "serverless-offline": "^13.8.0"
  }
}
